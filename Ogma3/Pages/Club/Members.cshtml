@page "{id}"
@model Ogma3.Pages.Club.Members

@{
    ViewData["Title"] = Model.ClubBar.Name;
    ViewData["Tab"] = "Members";
}

<partial name="_ClubBar" model="@Model.ClubBar"/>

<ul class="two-columns o-grid">
    @foreach (var m in Model.ClubMembers)
    {
        <partial name="Shared/_UserCard" model="m"/>
    }
</ul>