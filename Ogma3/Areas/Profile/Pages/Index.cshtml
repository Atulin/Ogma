@page "{name?}"
@model Ogma3.Areas.Profile.Pages.Index

@{
    ViewData["Title"] = $"{User.Identity.Name}'s Profile";
    ViewData["User"] = Model.CurrentUser;
}

<markdown preset="All">
    @Model.CurrentUser.Bio
</markdown>

<hr>

<partial name="_Comments" model="@Model.CurrentUser.CommentsThread"></partial>