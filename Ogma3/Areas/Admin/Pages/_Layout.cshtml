@{
    Layout = "/Pages/Shared/_Layout.cshtml";
}

<h1>Admin panel</h1>

<div>
    <h4>@ViewData["Title"]</h4>
    <hr />
    <div class="row">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9" id="app">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
